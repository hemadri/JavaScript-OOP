package com.flexutil.shell.events
{
	
	import flash.events.Event;

	public class ViewStackEvent extends Event
	{
		public var menuOperation:String;
		
		public static const VIEW_CHANGED:String = 'viewChanged';
		
		public function ViewStackEvent( type:String, menuOperation:String )
		{
			super( type, true );
			this.menuOperation = menuOperation;
		}
	}
}