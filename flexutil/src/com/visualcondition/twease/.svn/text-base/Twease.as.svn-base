 && k != 'propcount' && tweens[h][k].active) ust(tweens[h][k]);										updater.addEventListener(Event.ENTER_FRAME, update);					gctimer = new Timer(collectionrate);					gctimer.addEventListener(TimerEvent.TIMER, garbagecollect);					gctimer.start();				}				else {					updater.removeEventListener(Event.ENTER_FRAME, update);					gctimer.stop();					gctimer.removeEventListener(TimerEvent.TIMER, garbagecollect);								}				active = setactive;			} else {				if(prop == null){					if(setactive) for ( var w:Object in tweens[target] ) if(w != 'active' && w != 'propcount' && tweens[target][w].active) ust(tweens[target][w]);					tweens[target].active = setactive;				} else {					if(setactive && !tweens[target][prop].active) ust(tweens[target][prop]);					tweens[target][prop].active = setactive;				}			}			activetweens = new Dictionary();			for ( var j:Object in tweens ) {				if(tweens[j].active) {					activetweens[j] = {};					for ( var i:String in tweens[j] ) if(i != 'active' && i != 'propcount' && tweens[j][i].active) activetweens[j][i] = true;				}			}					};				private static function garbagecollect(evt):void {			var tc:Number = 0;			for ( var i:Object in tweens ){				tc++;				if(tweens[i].propcount == 0) {delete tweens[i]; delete activetweens[i]};			};			if(tc == 0) setActive(false);		}				public static function advance(id:Number, position:* = null):* {			var np:Number = queue[id].position = (position == null) ? ++queue[id].position : position;			if(queue[id].length-1 < np) queue[id] = new Array();			if(queue[id].length == 0){				for ( var i:String in queue ) if(queue[i].length != 0) return null;				queue = new Array();			} else {				var o:Object = {};				for ( var r:String in  queue[id][np]) o[r] = queue[id][np][r];				if(queue[id].target != undefined) {					var oc:Number = 0;					for ( var l:String in o ) oc++;					if(o.func != undefined && o.target == undefined && oc <= 2){}					else o.target = queue[id].target;				}				o.queue = [id, np];				tween(o);			}			return id;		};				public static function compareInObject(prop, cont):Boolean {			for ( var i in cont ) if(cont[i] == prop) return true;			return false;		};				public static function none(t:Number, b:Number, c:Number, d:Number, ... extras):Number { return c*t/d+b; }		public static function easeIn(t:Number, b:Number, c:Number, d:Number, ... extras):Number { return c*(t /= d)*t*t*t*t+b; }		public static function easeOut(t:Number, b:Number, c:Number, d:Number, ... extras):Number { return c*((t=t/d-1)*t*t*t*t+1)+b; }				public static function render(tweenholder:Array, gtt:Number):Boolean{			var o:* = tweenholder[0];			var tmr:Number = (gtt+o.progdif)-o.starttime;			var dn:Boolean;			if(o.rate != undefined) dn = (o.rateleft <= 0) ? true : false;			else dn = (tmr >= (o.time+o.delay)) ? true : false;			if(dn) {				if(o.newval != undefined) o.target[o.prop] = (o.round) ? int(o.newval) : o.newval;				o.cycles--;				if(o.func != undefined) o.func(o.target, o.prop, (o.queue != undefined) ? o.queue[0] : null);				if(o.cycles == 0){					tweenholder.shift();					if(o.queue != undefined) if(queue[o.queue[0]].position == o.queue[1] && queue[o.queue[0]].position != queue[o.queue[0]].length-1) advance(o.queue[0], o.queue[1]+1);				}				var ocyc:Number = o.cycles;				if(tweenholder[0] != undefined){					o = tweenholder[0];					if(o.oldelay != 0 && o.oldelay != undefined) o.delay = o.oldelay;					o.starttime = gtt;					if(o.newval != undefined){						o.value = (ocyc != 0) ? (typeof(o.value) == 'string') ? String(-1*o.value) : o.startpos : o.value;						o.startpos = o.target[o.prop];						o.newval = (typeof(o.value) == 'string') ? o.startpos + Number(o.value) : o.value;						o.dif = (o.startpos > o.newval) ? (o.newval-o.startpos) : -(o.startpos-o.newval);						o.rate = (o.rate != undefined) ? -1*o.rate : undefined;						o.rateleft = (o.dif < 0) ? -o.dif : o.dif;						o.bezier.reverse();					}				} else return true;			} else {				if(o.delay == 0){					if(o.startfunc != undefined){						o.startfunc(o.target, o.prop, (o.queue != undefined) ? o.queue[0] : null);						delete o.startfunc;					}					var res:Number, nres:Number;					if(o.rate == undefined) res = easeProxy(o.ease, tmr, o.startpos, o.dif, o.time, o.extra1, o.extra2);					else{						var rlr:Number = (o.rateleft/o.rate);						res = (o.newval-(((rlr < 0) ? -rlr : rlr)*o.rate));						o.rateleft -= (o.rate < 0) ? -o.rate : o.rate;					}					o.easeposition = (res-o.startpos)/(o.newval-o.startpos);					if(o.bezier.length < 1) nres = res;					else if(o.bezier.length == 1)nres = o.startpos + (o.easeposition*(2*(1-o.easeposition)*(o.bezier[0]-o.startpos)+(o.easeposition*o.dif)));					else {						var b1:Number, b2:Number;						var bpos:Number = int(o.easeposition*o.bezier.length);						var ipos:Number = (o.easeposition-(bpos*(1/o.bezier.length)))*o.bezier.length;						if (bpos == 0){							b1 = o.startpos;							b2 = (o.bezier[0]+o.bezier[1])*.5;						} else if (bpos == o.bezier.length-1){							b1 = (o.bezier[bpos-1]+o.bezier[bpos])*.5;							b2 = o.newval;						} else{							b1 = (o.bezier[bpos-1]+o.bezier[bpos])*.5;							b2 = (o.bezier[bpos]+o.bezier[bpos+1])*.5;						}						nres = b1+ipos*(2*(1-ipos)*(o.bezier[bpos]-b1) + ipos*(b2 - b1));					}					o.target[o.prop] = (o.round) ? int(nres) : nres;					if(o.upfunc != undefined) o.upfunc(o.target, o.prop, (o.queue != undefined) ? o.queue[0] : null);				} else {					if(gtt >= o.starttime+o.delay){						o.oldelay = o.delay;						o.delay = 0;						o.starttime = gtt;						var ftv = o.target[o.prop];						var newval:Number = (typeof o.value == 'string') ? ftv + Number(o.value) : o.value;						var dif:Number = (ftv > newval) ? (newval-ftv) : -(ftv-newval);						var bzarr:Array = new Array();						if(o.oldbezier != undefined){							var beza:Array = (o.oldbezier is Array) ? o.oldbezier : [o.oldbezier];							if(beza.length > 0){ for ( var b in beza ){ if(beza[b][o.prop] != undefined) bzarr.push((typeof beza[b][o.prop] == 'string') ? ftv + Number(beza[b][o.prop]) : beza[b][o.prop]);};}						}						o.rate = (o.rate != undefined) ? ((ftv > newval) ? -1*o.rate : o.rate) : undefined;						o.newval = newval;						o.dif = dif;						o.startpos = ftv;						o.bezier = bzarr;						o.rateleft = (dif < 0) ? -dif : dif;						if(o.rate != undefined){							o.rateleft -= o.rateleft*o.startprogress;							o.target[o.prop] = o.startpos += o.dif *= o.startprogress;						} else o.progdif = o.time*o.startprogress;					}				}				o.lasttime = gtt;			}			return false;		};				public static function update(timerevent:Event):void {			var gtt:Number = getTimer();			for (var i:Object in activetweens){				for (var j:String in activetweens[i]){					var ddi:Boolean;					var nst:Object = tweens[i][j][0];					if(nst.tweenobject != undefined){						ddi = true;						for ( var c in nst.tweenobject ){							if(nst.tweenobject[c] is Array){								if(render(nst.tweenobject[c], gtt)) delete nst.tweenobject[c];							} else delete nst.tweenobject[c];							ddi = false;						}						if(ddi){							if(tweens[i][j][1] != undefined){								ddi = false;								tweens[i][j].shift();							}						}						nst.applyfunc(nst);					} else ddi = (i == 'functions' || tweens[i][j][0].target.hasOwnProperty(j)) ? render(tweens[i][j], gtt) : true;					if(ddi){						delete tweens[i][j];						delete activetweens[i][j];						tweens[i].propcount--;					}				}			}		};				public static function tween(ao:Object, tovr:Object = null, nonm:* = false):Object {			if(ao[0] == undefined){				if(active == false) setActive(true);				var delay:Number = (ao.delay == undefined || ao.delay < 0.1) ? 0.1 : ao.delay*1000;				var ncycles:Number = (ao.cycles == undefined) ? 1 : ao.cycles;				var snt:Number = getTimer();				var tg:Object;				var tarr:Array;				var prop:String;				if(ao.func != undefined && ao.target == undefined){					tg = (tweens.functions == undefined) ? tweens.functions = {propcount:0} : tweens.functions;									if(tg.active == undefined){						tg.active = true;						activetweens.functions = new Object();					}					prop = "func" + (int(Math.random()*100000)).toString();					tarr = tg[prop] = new Array();					tarr.active = true;					activetweens.functions[prop] = true;					tg.propcount++;					tarr.push({prop:prop, starttime:snt, time:0, func:ao.func, delay:delay, queue:ao.queue, cycles:ncycles, progdif:0});				} else {					var ntarg:Object = (tovr == null) ? ao.target : tovr;					var dostack:Boolean = (ao.stack != undefined) ? ao.stack : stacking;					var ease:Function;					if(extensions.Easing != undefined){						if(ao.ease == undefined) ease = extensions.Easing.linear;						else ease = (typeof ao.ease == 'string') ? extensions.Easing[ao.ease] : ao.ease;					} else {ease = (ao.ease == undefined) ? none : ao.ease;}					if(nonm === true) tg = {propcount:0, active:true};					else if(typeof nonm == 'object') tg = nonm;					else {						tg = (tweens[ntarg] == undefined) ? tweens[ntarg] = {propcount:0} : tweens[ntarg];						if(tg.active == undefined){							tg.active = true;							activetweens[ntarg] = new Object();						}					}					for( var i:String in ao ){						if(compareInObject(i, baseprops)){}						else if(Twease.extendedprops != null && extensions.Extend.checkExtendedProp(i)){ extensions.Extend.propSetup(Twease.extendedprops[i], ao); }						else {							var newa:Array = new Array();							if(i == 'array') for ( var g:String in ao[i] ) newa[g] = {index:[g,ao[i][g]]};							else {								var tomk:Object = new Object();								tomk[i] = ao[i];								newa.push(tomk);							}							for ( var q:String in newa ){								for ( var s:String in newa[q] ){									prop = (s == 'index') ? newa[q][s][0].toString() : i;									var value = (s == 'index') ? newa[q][s][1] : ao[i];									if(isNaN(value)) continue;									if(tg[prop] != undefined && !dostack) tg.propcount--;									tarr = (tg[prop] == undefined || !dostack) ? tg[prop] = new Array() : tg[prop];									if(tarr.active == undefined) {										tarr.active = true;										tg.propcount++;										if(nonm != true) activetweens[ntarg][prop] = true;									}									tarr.push({target:ntarg, cycles:ncycles, prop:prop, ease:ease, starttime:snt, queue:ao.queue, startpos:prop, value:value, time:(ao.time == undefined && ao.rate == undefined) ? 0 : ao.time*1000, rate:ao.rate, func:ao.func, startfunc:ao.startfunc, upfunc:ao.upfunc, round:(ao.round == undefined) ? roundresults : ao.round, delay:delay, extra1:ao.extra1, extra2:ao.extra2, oldbezier:ao.bezier, easeposition:null, startprogress:(ao.progress == undefined) ? 0 : ao.progress, progdif:0});								};							};						}					};				}				return tg;			} else {				var nqp:Number = queue.push(ao)-1;				queue[nqp].target = tovr;				return advance(nqp, 0);			}		}				private static function easeProxy(ef:Function, a:Number, b:Number, c:Number, d:Number, e:* = null, f:* = null):Number{			if(e == null) return ef.apply(null, arguments.slice(1,5));			else return ef.apply(null, arguments.slice(1));		}	}}