<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark" 
		width="12" height="12"
		alpha.disabled="0.5" toolTip="Remove">

	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>

	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>

	<s:BitmapImage 
		source.up="@Embed(source='assets/icons/edit.png')"  
		source.over="@Embed(source='assets/icons/edit.png')"  
		source.down="@Embed(source='assets/icons/edit.png')"  />
	
</s:SparkSkin>
